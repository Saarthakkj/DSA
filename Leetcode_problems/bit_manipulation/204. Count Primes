class Solution {
public:
    int countPrimes(int n) {
        vector<int> mpp(5*pow(10,6) +1, 0);
        for(long long int i = 2 ; i<=n ; i++){
            if(mpp[i] == 0){
                for(long long int j =(long long) i*i ; j <=n ; j+=i){
                    //cout<< "j<=n " <<  j ;
                    mpp[j] = 1;
                }
            }
        }
        int count = 0 ;
        for(long long int k = 2 ; k <n ; k++){
            if(mpp[k] == 0){
                count++;
            }
        }

        return count ;
    }
};
